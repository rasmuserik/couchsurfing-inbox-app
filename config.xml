<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="dk.rasmuserik.mobile.cs" version="0.0.1">
<name short="CS-client">CouchSurfing Client</name>

<description>Mobile access to CouchSurfing.org</description>

<author href="http://www.rasmuserik.dk/">Rasmus Erik</author>
<icon src="icon.png" gap:role="default" />
<gap:splach src="splash.png" gap:role="default" />

<gap:platforms>
<gap:platform name="android" minVersion="1.6" />
<gap:platform name="webos" />
<gap:platform name="symbian.wrt" />
<gap:platform name="blackberry" project="widgets"/>
</gap:platforms>


<feature name="http://api.phonegap.com/1.0/network" required="true"/>

<preference name="autorotate" value="true" readonly="false"/>
</widget>
